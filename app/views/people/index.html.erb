<h1 class="ui header myHeader">People</h1>

<table class="ui celled table">
  <thead>
    <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone</th>
    <th>Avatar</th>
    <th>Email</th>
  </tr>
  </thead>
  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td><%= person.first_name %></td>
        <td><%= person.last_name %></td>
        <td><%= person.phone %></td>
        <td><%= person.avatar %></td>
        <td><%= person.email%></td>
      </tr>
    <% end %>
  </tbody>
</table>
